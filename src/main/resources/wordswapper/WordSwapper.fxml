<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="10" alignment="CENTER" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.example.wordswapper.WordSwapperController">
  <TextField fx:id="textField1" promptText="Введите текст здесь"/>
  <Button fx:id="swapButton" text="→" onAction="#swapText"/>
  <TextField fx:id="textField2" promptText="Результат"/>
</VBox>